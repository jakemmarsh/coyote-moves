<?xml version="1.0"?>
<configuration>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
  </startup>
  <connectionStrings>
    <add name="DefaultConnection" connectionString="Data Source = |SQL/CE|" />
  </connectionStrings>
  <system.serviceModel>
    <bindings>
      <basicHttpBinding>
        <binding name="AXLAPIBinding">
          <security mode="Transport" />
        </binding>
        <binding name="AXLAPIBinding1" />
        <binding name="AXLAPIBinding2">
          <security mode="Transport" />
        </binding>
        <binding name="AXLAPIBinding3" />
      </basicHttpBinding>
    </bindings>
    <client>
      <endpoint address="https://CCMSERVERNAME:8443/axl/" binding="basicHttpBinding" bindingConfiguration="AXLAPIBinding" contract="ServiceReference1.AXLPort" name="AXLPort" />
      <endpoint address="https://CCMSERVERNAME:8443/axl/" binding="basicHttpBinding" bindingConfiguration="AXLAPIBinding2" contract="AXLAPI.AXLPort" name="AXLPort1" />
    </client>
  </system.serviceModel>
</configuration>